<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:ser="http://schemas.microsoft.com/2003/10/Serialization/" xmlns:tns="http://schemas.datacontract.org/2004/07/ExtWatcher.Common.Contract" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/ExtWatcher.Common.Contract" xmlns:xs="http://www.w3.org/2001/XMLSchema">
